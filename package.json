{
  "name": "hack-pracuj-backend",
  "version": "1.0.0",
  "description": "Provide backend for pracuj hack",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "compile": "tsc -w -p tsconfig.json",
    "dev": "nodemon --max-http-header-size 115000 --watch dist/index.js dist/index.js",
    "start": "node --max-http-header-size 115000 dist/index.js",
    "docker:build": "docker build -t soft-backend .",
    "docker:run": "docker run -d -p 3030:3030 soft-backend",
    "format": "prettier --write './**/*.{ts,tsx,css,scss}' --config ./.prettierrc",
    "format:ci": "prettier --check './**/*.{ts,tsx,css,scss}' --config ./.prettierrc"
  },
  "dependencies": {
    "@fastify/busboy": "^2.1.1",
    "@fastify/caching": "^8.3.0",
    "@fastify/compress": "^7.0.3",
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/env": "^4.3.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/mongodb": "^8.0.0",
    "@fastify/multipart": "^8.2.0",
    "@fastify/rate-limit": "^9.1.0",
    "@fastify/session": "^10.8.0",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@fastify/websocket": "^10.0.1",
    "@scalar/fastify-api-reference": "^1.22.37",
    "@sinclair/typebox": "^0.32.30",
    "apitally": "^0.4.0",
    "awilix": "^10.0.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.11",
    "fastify": "^4.27.0",
    "fastify-crons": "^0.1.0",
    "fastify-plugin": "^4.5.1",
    "node-ssh": "^13.2.0",
    "nodemailer": "^6.9.13",
    "openai": "^4.47.1",
    "raw-body": "^2.5.2",
    "sharp": "^0.33.3",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@tsconfig/node20": "^20.1.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.12.12",
    "@types/ua-parser-js": "^0.7.39",
    "@types/ws": "^8.5.10",
    "audit-ci": "^6.6.1",
    "nodemon": "^3.1.0",
    "plop": "^4.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "pre-commit": [
    "lint",
    "format"
  ]
